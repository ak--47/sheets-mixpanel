{
	"tabWidth": 4,
	"useTabs": false,
	"printWidth": 200,
	"arrowParens": "avoid",
	"trailingComma": "none",
	"embeddedLanguageFormatting": "off",
	"overrides": [
		{
			"files": [
				"package-lock.json",
				"package.json"
			],
			"options": {
				"requirePragma": true,
				"parser": "json"
			}
		}
	]
}